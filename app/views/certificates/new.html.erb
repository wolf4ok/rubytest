<h2> new Certificates </h2> </br>

<%= form_tag certificates_path, method: :post do %>
  <%= label_tag :user_id %>
  <%= text_field_tag :user_id %>
  <%= label_tag :cert_id %>
  <%= text_field_tag :cert_id %>
  <%= label_tag :number %>
  <%= text_field_tag :number %>
  <%= label_tag :data %>
  <%= date_field_tag :data %>
  <%= label_tag :file %>
  <%= text_field_tag :file %>
  <%= submit_tag "Save", class: "btn btn-warning" %>
<% end %>